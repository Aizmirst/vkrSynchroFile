<Window x:Class="vkrSynchroFile.ProfileInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vkrSynchroFile"
        mc:Ignorable="d"
        Title="Подробнее о профиле" Height="400" Width="370" MinHeight="400" MinWidth="370">
    <Grid>
        <TextBlock HorizontalAlignment="Center" TextAlignment="Center" Margin="0,20,0,0" Name="profTitle" Text="Параметры профиля" VerticalAlignment="Top" Width="220" FontSize="14"/>
        <TextBlock HorizontalAlignment="Center" TextAlignment="Left" Margin="0,60,0,0" TextWrapping="Wrap" Text="" Name="profInfo" VerticalAlignment="Top" Width="260"/>
    </Grid>
</Window>
