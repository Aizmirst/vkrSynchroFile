<Window x:Class="vkrSynchroFile.InputUserIDWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vkrSynchroFile"
        mc:Ignorable="d"
        Title="InputUserIDWindow" Height="123" Width="300" MinHeight="123" MinWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" x:Name="InputTextBox" MaxLength="36" Margin="20,0,20,0" Height="20" PreviewTextInput="MaskedTextBox_PreviewTextInput"/>
        <Button Grid.Row="1" Width="80" Height="25" Content="Отправить" Click="SendButton_Click" />
    </Grid>
</Window>
